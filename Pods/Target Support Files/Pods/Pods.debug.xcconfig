ARCHS = $(ARCHS_STANDARD)
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/AMapNavi" "${PODS_ROOT}/AMapSearch" "${PODS_ROOT}/BmobSDK"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/AMapNavi" "${PODS_ROOT}/Headers/Public/AMapSearch" "${PODS_ROOT}/Headers/Public/BmobSDK" "${PODS_ROOT}/Headers/Public/Mantle"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AMapNavi" -isystem "${PODS_ROOT}/Headers/Public/AMapSearch" -isystem "${PODS_ROOT}/Headers/Public/BmobSDK" -isystem "${PODS_ROOT}/Headers/Public/Mantle"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Mantle" -l"icucore" -l"sqlite3" -l"stdc++.6.0.9" -l"z" -l"z.1.2.5" -framework "AMapNaviKit" -framework "AMapSearchKit" -framework "BmobSDK" -framework "CFNetwork" -framework "CoreGraphics" -framework "CoreLocation" -framework "CoreTelephony" -framework "Foundation" -framework "MobileCoreServices" -framework "Security" -framework "SystemConfiguration"
PODS_ROOT = ${SRCROOT}/Pods